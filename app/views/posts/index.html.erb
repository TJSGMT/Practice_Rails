<div class="container"><% if current_user %>
  <%= link_to "Log-Out", destroy_user_session_path, class:"btn btn-danger", data: {turbo_method: :delete} %><br>
<% end %>
<h1>Posts</h1>
<div id="posts">
  <% @posts.each do |post| %>
    <%= render post %>
    <p>
      <%= link_to "Show this post", post %>
    </p>
  <% end %>
</div>
<%= link_to "New post", new_post_path %>
