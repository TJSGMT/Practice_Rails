<li>
  <strong> <%= place.title%></strong><br>
    <% if place.addresses.any? %>
      Addresses:
      <ul>
        <% place.addresses.each do |addr|%>
          <li>
            <%= addr.city%>,<%= addr.street%>
            <strong><%= place.title%></strong> | <%= link_to 'Edit place',edit_place_path(place)%> 
          </li>
        <%end%>
      </ul> 
    <%end%>       
</li>